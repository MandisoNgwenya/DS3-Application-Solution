
@model Microsoft.AspNet.Identity.EntityFramework.IdentityRole




<style type="text/css">
    #tbrole, .c {
        border: double;
    }
</style>

@using (Html.BeginForm())
{
    <table class="table table-bordered table-hover">
        <tr>
            <td class="c">Sytem Role Name</td>
            <td class="c">
