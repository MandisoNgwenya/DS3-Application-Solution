
@{
    ViewBag.Title = "Device";
}

<div class="form-group pull-in clearfix">

    <div class="col col-lg-12">



        <div class="wrapper-xl" align="left">

            <div class="row">
                <div class="col-lg-12">

                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <span class="h4">

                            </span>
                        </div>
                        <div class="panel-body">

                            <p class="text-muted">Booking/Device</p>
                            <div class="form-group">
                                <label>Hello:@TempData["Title"] @TempData["Name"] @TempData["Surname"]   <em class="text-muted">is this your correct Identity Number?  @Session["IdentityNumber"], please enter relevant device information for your     @TempData["Device"]</em></label>
                                
                            </div>

                            <div class="form-group pull-in clearfix">


                                <div class="col col-sm-6">

                                    <div class="panel panel-danger">
                                        <div class="panel-heading">
                                            <span class="h4">
                                                Add Device 
                                            </span>
                                        </div>
                                        <div class="panel-body active ">
                                            <p class="text-muted"></p>
                                            
                                            @Html.Partial("_AddDevice")

                                        </div>
                                    </div>
                                </div>


                                <div class="col col-sm-6">

                                    <div class="panel panel-warning">
                                        <div class="panel-heading">
                                            <span class="h4">
                                                Booking Information
                                            </span>
                                        </div>
                                        <div class="panel-body active ">
                                            <p class="text-muted">Enter Device Information</p>
                                            @Html.Partial("_Finish")


                                        </div>
                                    </div>
                                </div>
                   



                            </div>

                       
                    </div>

                </div>
              

            </div>

        </div>

    </div>
</div>

    </div>