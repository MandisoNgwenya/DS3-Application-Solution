@model IEnumerable<Application.ClientUI.Models.BookingViewModel>

@{
    ViewBag.Title = "Index";
}



<table class="table">
    <tr>


        <th>
            @Html.DisplayNameFor(model => model.SerialNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Device)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Technician)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JobCard)
        </th>


        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SerialNo)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Device)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Technician)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobCard)
            </td>
            <td>

            <td>
                @Html.ActionLink("", "Edit", new { id = item.BookingID }) |
                @Html.ActionLink("Details", "Details", new { id = item.BookingID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.BookingID })
            </td>
        </tr>
    }

</table>

