<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                @Session["IdentityNumber"]
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <table class="table table-striped table-bordered table-hover">

                    <tbody>


                        <tr class="info">


                            <th>
                                @Html.DisplayNameFor(model => model.Device)


                            </th>



                            <th>
                                @Html.DisplayNameFor(model => model.JobCard)
                            </th>


                        </tr>

                        @foreach (var item in Model)
                        {
                            <tr class="success">


                                <td>
                                    <p> @Html.DisplayFor(modelItem => item.Device)  |  Date: @Html.DisplayFor(modelItem => item.Date)</p>

                                    <p> @Html.ActionLink("Status", "Status")   |  @Html.ActionLink("Details", "Quotes","Devices" ) </p>
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.JobCard)


                                    <p> @Html.ActionLink("View Quotation", "")  </p>
                                </td>



                            </tr>
                        }
                    </tbody>
                </table>

            </div>
        </div>
    </div>
